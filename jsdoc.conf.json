{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "./documentation-template",
    "readme": "./readme.md"
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": ["./application-workflow.png"]
      }
    }
  }
}
