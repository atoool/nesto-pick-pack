{
  "name": "NestoPickPack",
  "version": "2.2.18",
  "buildNumber": "4",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "set:env:dev": "node ./scripts/set-env.js --env dev",
    "set:env:staging": "node ./scripts/set-env.js --env staging",
    "set:env:production": "node ./scripts/set-env.js --env production",
    "move:apk:dev": "mv android/app/build/outputs/apk/release/app-release.apk ~/Desktop/$npm_package_name-$npm_package_version-$npm_package_buildNumber-$npm_package_env-dev.apk",
    "move:apk:staging": "mv android/app/build/outputs/apk/release/app-release.apk ~/Desktop/$npm_package_name-$npm_package_version-$npm_package_buildNumber-$npm_package_env-staging.apk",
    "move:apk:production": "mv android/app/build/outputs/apk/release/app-release.apk ~/Desktop/$npm_package_name-$npm_package_version-$npm_package_buildNumber-$npm_package_env-production.apk",
    "move:bundle:production": "mv android/app/build/outputs/bundle/release/app-release.aab ~/Desktop/$npm_package_name-$npm_package_version-$npm_package_buildNumber-$npm_package_env-production.aab",
    "upver": "node ./upver.js",
    "test": "jest",
    "lint": "eslint .",
    "build:android:dev": "yarn set:env:dev && cd android && ./gradlew assembleRelease && cd .. && yarn move:apk:dev",
    "run:android:dev": "yarn set:env:dev && yarn android --variant=release && yarn move:apk:dev",
    "run:android:dev:debug": "yarn set:env:dev && yarn android",
    "build:android:staging": "yarn set:env:staging && cd android && ./gradlew assembleRelease && cd .. && yarn move:apk:staging",
    "run:android:staging": "yarn set:env:staging && yarn android --variant=release && yarn move:apk:staging",
    "run:android:staging:debug": "yarn set:env:staging && yarn android",
    "build:android:production": "yarn set:env:production && cd android && ./gradlew assembleRelease && cd .. && yarn move:apk:production",
    "build:bundle:production": "yarn set:env:production && cd android && ./gradlew bundleRelease && cd .. && yarn move:bundle:production",
    "run:android:production": "yarn set:env:production && yarn android --variant=release && yarn move:apk:production",
    "run:android:production:debug": "yarn set:env:production && yarn android",
    "build:android:all": "yarn build:android:dev && yarn build:android:staging && yarn build:android:production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.3",
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-firebase/app": "^10.5.0",
    "@react-native-firebase/crashlytics": "^10.5.1",
    "@react-native-firebase/messaging": "^10.5.1",
    "@react-navigation/bottom-tabs": "^5.11.3",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "axios": "^0.21.1",
    "moment-timezone": "^0.5.33",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-barcode-builder": "^2.0.0",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-device-info": "^8.0.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-localize": "^2.0.1",
    "react-native-paper": "^4.7.1",
    "react-native-paper-dropdown": "^0.0.5",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-restart": "^0.0.20",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-svg": "^12.1.0",
    "react-native-thermal-receipt-printer": "^1.1.4",
    "react-native-vector-icons": "^8.0.0",
    "react-native-view-shot": "^3.1.2",
    "react-native-webview": "^11.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.18.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "replace-in-file": "^6.2.0",
    "yargs": "^16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
